{
  "name": "recoil",
  "version": "0.6.10",
  "displayName": "Recoil",
  "description": "Dark color theme for modern systems languages such as Rust and Zig",
  "license": "MIT",
  "publisher": "ellacrity",
  "keywords": [
    "rust",
    "zig",
    "toml",
    "json",
    "yaml",
    "colorful",
    "semantic-tokens",
    "theme",
    "codium",
    "dark",
    "wcag",
    "accessibility"
  ],
  "categories": [
    "Themes"
  ],
  "extensionKind": [
    "ui"
  ],
  "icon": "./assets/images/logo.png",
  "homepage": "https://github.com/ellacrity/recoil-vscodium-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ellacrity/recoil-vscodium-theme"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "scripts": {
    "publish": "npx ovsx publish --pat $OPEN_VSX_TOKEN",
    "vsce-build": "npx vsce package",
    "publish-all": "npm run vsce-publish && npm run ovsx-publish",
    "vsce-publish": "npx vsce publish --pat $VS_MARKETPLACE_TOKEN",
    "ovsx-publish": "npx ovsx publish --pat $OPEN_VSX_TOKEN"
  },
  "contributes": {
    "themes": [
      {
        "label": "Recoil",
        "uiTheme": "vs-dark",
        "path": "/themes/Recoil-color-theme.json"
      }
    ]
  }
}
